#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/dist/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/dist/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules:/Volumes/Data/Projects/personal/emextools/email-verifier-plus-npm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2/node_modules/ts-node/dist/bin.js" "$@"
fi
